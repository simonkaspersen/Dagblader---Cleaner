{
  "name": "Dagbladet - Commonizer",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Fjern alle sensasjonelle banner og farger",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "host_permissions": [
    "https://www.dagbladet.no/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.dagbladet.no/"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}